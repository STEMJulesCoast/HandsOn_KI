    "def check_num(num):\n",
    "    if num > 0:\n",
    "        return \"positive\"\n",
    "    elif num < 0:\n",
    "        return \"negative\"\n",
    "    else:\n",
    "        return \"zero\"\n",
    "\n",
    "#print results\n",
    "print(check_num(10))  # should be positive\n",
    "print(check_num(-5))  # should be negative\n",
    "print(check_num(0))   # should be \"zero\"\n"
   ]
  }
